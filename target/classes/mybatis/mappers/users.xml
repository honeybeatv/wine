<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="users">

<select id="getList" > 
		<![CDATA[	
		SELECT NO , 
			   NAME, 
			   PASSWORD , 
			   CONTENT , 
			   REG_DATE 
		  FROM GUESTBOOK 
		 ORDER BY NO DESC
		]]>
	</select>
</mapper>